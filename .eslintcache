 this.addToCart(furniture.product_id)}>Add</button>\n             </article>\n             </div>\n            )) }\n            {/* <Checkout\n            order_item={this.state.order_item} */}\n            {/* // addToCart ={this.addToCart} */}\n            \n        </div>\n    )\n  }    \n}\nexport default Furniture;","/Users/janellebaguley/Documents/DevMountain/unit-7/blush-resto-design/src/redux/cartReducer.js",[],"/Users/janellebaguley/Documents/DevMountain/unit-7/blush-resto-design/src/components/Checkout/UpdateCart.js",["74"],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":1,"column":16,"nodeType":"81","messageId":"82","endLine":1,"endColumn":25},{"ruleId":"79","severity":1,"message":"83","line":2,"column":9,"nodeType":"81","messageId":"82","endLine":2,"endColumn":13},{"ruleId":"79","severity":1,"message":"84","line":4,"column":8,"nodeType":"81","messageId":"82","endLine":4,"endColumn":12},{"ruleId":"85","severity":1,"message":"86","line":39,"column":66,"nodeType":"87","messageId":"88","endLine":39,"endColumn":70},{"ruleId":"79","severity":1,"message":"89","line":8,"column":8,"nodeType":"81","messageId":"82","endLine":8,"endColumn":13},{"ruleId":"79","severity":1,"message":"89","line":4,"column":8,"nodeType":"81","messageId":"82","endLine":4,"endColumn":13},{"ruleId":"90","severity":1,"message":"91","line":11,"column":6,"nodeType":"92","endLine":11,"endColumn":24,"suggestions":"93"},{"ruleId":"79","severity":1,"message":"94","line":4,"column":8,"nodeType":"81","messageId":"82","endLine":4,"endColumn":16},{"ruleId":"95","severity":1,"message":"96","line":70,"column":15,"nodeType":"97","endLine":70,"endColumn":60},{"ruleId":"79","severity":1,"message":"89","line":4,"column":8,"nodeType":"81","messageId":"82","endLine":4,"endColumn":13},"no-native-reassign",["98"],"no-negated-in-lhs",["99"],"no-unused-vars","'Component' is defined but never used.","Identifier","unusedVar","'Link' is defined 