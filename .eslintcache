>\n                          <input value = {this.state.verPassword}\n                          name='verPassword'\n                          placeholder = 'Verify Password'\n                          onChange={e => this.handleInput(e)}/>\n                          <button onClick={this.handleRegister}>Register</button><p>Have an account? <span onClick={this.handleToggle}>Login here</span></p>\n                    </div>\n                )\n                : (\n                <div>\n                    <button onClick ={this.handleLogin}>Login</button>\n                    <p>Don't have an account<span onClick={this.handleToggle}>Register here</span></p>\n                </div>)}\n                <section>\n                <h3>{this.props.user.email}</h3>\n                <button onClick={this.handleLogout}>Logout</button>\n                </section>\n                    \n                </section>\n            </div>\n        )\n    }\n}\nconst mapStateToProps = reduxState => reduxState;\n\nexport default connect(mapStateToProps, {getUser})(Auth);","/Users/janellebaguley/Documents/DevMountain/unit-7/blush-resto-design/src/components/Header.js",["42"],"import {withRouter, Link} from 'react-router-dom';\n\nconst Header = props => {\n    return (\n        <div>Blush</div>\n    )\n}\nexport default withRouter(Header)",{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","replacedBy":"46"},{"ruleId":"47","severity":1,"message":"48","line":3,"column":8,"nodeType":"49","messageId":"50","endLine":3,"endColumn":12},{"ruleId":"47","severity":1,"message":"51","line":5,"column":9,"nodeType":"49","messageId":"50","endLine":5,"endColumn":18},{"ruleId":"47","severity":1,"message":"52","line":1,"column":21,"nodeType":"49","messageId":"50","endLine":1,"endColumn":25},"no-native-reassign",["53"],"no-negated-in-lhs",["54"],"no-unused-vars","'Auth' is defined but never used.","Identifier","unusedVar","'clearUser' is defined but never used.","'Link' is defined but never used.","no-global